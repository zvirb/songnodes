{
  "name": "songnodes-dj",
  "version": "2.0.0",
  "description": "DJ-focused music graph visualization for setlist construction",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "playwright test",
    "test:graph": "playwright test --project=graph-visualization",
    "test:webgl-stress": "playwright test --project=webgl-stress-test",
    "test:pixi": "playwright test --project=pixi-compatibility",
    "test:performance": "playwright test --project=performance",
    "test:visual": "playwright test --project=visual-regression",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "test:install": "playwright install",
    "diagnose": "node diagnostic-script.js",
    "diagnose:system": "node diagnostic-script.js"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@pixi/react": "^7.1.2",
    "@types/react-window": "^1.8.8",
    "clsx": "^2.1.1",
    "comlink": "^4.4.1",
    "d3-array": "^3.2.4",
    "d3-axis": "^3.0.0",
    "d3-drag": "^3.0.0",
    "d3-force": "^3.0.0",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.1.0",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-zoom": "^3.0.0",
    "fuse.js": "^7.0.0",
    "pixi.js": "^8.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hotkeys-hook": "^4.5.1",
    "react-use": "^17.5.1",
    "react-window": "^2.1.1",
    "web-vitals": "^5.1.0",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@types/d3-array": "^3.2.2",
    "@types/d3-axis": "^3.0.6",
    "@types/d3-drag": "^3.0.7",
    "@types/d3-force": "^3.0.9",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-selection": "^3.0.10",
    "@types/d3-shape": "^3.1.7",
    "@types/d3-zoom": "^3.0.8",
    "@types/node": "^22.9.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "playwright": "^1.55.1",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4",
    "vite": "^5.4.0"
  }
}
